{
  // Live Sass Compiler
  "liveSassCompile.settings.formats": [
    // This is the default.
    {
        "format": "expanded",
        "extensionName": ".css",
        "savePath": "/assets/css",
    },
    {
        "format": "compressed",
        "extensionName": ".min.css",
        "savePath": "/assets/css"
    }
  ],
  "liveSassCompile.settings.partialsList": [ "/**/_*.scss" ],
    "liveSassCompile.settings.autoprefix": [
      "> 1%",
      "last 20 versions"
    ],
    "liveSassCompile.settings.excludeList": [
        "**/node_modules/**",
        ".vscode/**"
    ]
}